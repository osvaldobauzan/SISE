        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <AssemblyName>CJF.Sgcpj.Judicatura.Tramite.Infrastructure</AssemblyName>
        <RootNamespace>CJF.Sgcpj.Judicatura.Tramite.Infrastructure</RootNamespace>
